{
    "name": "josepostiga/larabeat",
    "description": "A health-check endpoint for your Laravel applications",
    "keywords": ["php", "package", "laravel", "lumen", "health-check", "larabeat"],
    "homepage": "https://github.com/josepostiga/larabeat",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Postiga",
            "email": "iamjosepostiga@gmail.com",
            "homepage": "https://josepostiga.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "orchestra/testbench": "^5.4",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "JosePostiga\\Larabeat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosePostiga\\Larabeat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JosePostiga\\Larabeat\\LarabeatServiceProvider"
            ]
        }
    }
}
